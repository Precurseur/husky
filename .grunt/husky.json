{
    "namespace": "Husky",
    "requireJS": {
        "baseUrl": ".",
        "preserveLicenseComments": false,
        "paths": {
            "aura": "vendor/aura/lib",
            "underscore": "vendor/underscore/underscore",
            "eventemitter": "vendor/eventemitter2/eventemitter2",
            "backbone": "vendor/backbone/backbone",
            "requirejs": "vendor/requirejs/require",
            "text": "vendor/requirejs-text/text",
            "jquery": "vendor/jquery/jquery",
            "husky-validation": "vendor/husky-validation/validation",
            "globalize_lib": "vendor/globalize/globalize",
            "husky": "lib/husky",

            "__component__$navigation@husky": "husky_components/navigation/main",
            "__component__$header@husky": "husky_components/header/main",
            "__component__$button@husky": "husky_components/button/main",
            "__component__$datagrid@husky": "husky_components/datagrid/main",
            "__component__$dialog@husky": "husky_components/dialog/main",
            "__component__$dropdown@husky": "husky_components/dropdown/main",
            "__component__$matrix@husky": "husky_components/matrix/main",
            "__component__$select@husky": "husky_components/select/main",
            "__component__$auto-complete@husky": "husky_components/auto-complete/main"
        },
        "shim": {
            "backbone": {
                "exports": "Backbone",
                "deps": ["underscore", "jquery"]
            },
            "underscore": {
                "exports": "_"
            }
        },
        "include": [
            "husky",
            "jquery",
            "requirejs",
            "underscore",
            "backbone",
            "text",
            "aura/ext/debug",
            "aura/ext/mediator",
            "aura/ext/components",
            "husky-validation",
            "globalize_lib",

            "__component__$navigation@husky",
            "__component__$header@husky",
            "__component__$button@husky",
            "__component__$datagrid@husky",
            "__component__$dialog@husky",
            "__component__$dropdown@husky",
            "__component__$matrix@husky",
            "__component__$select@husky",
            "__component__$auto-complete@husky"
        ],
        "out": "dist/<%= PKG_VERSION %>/husky.js"
    }
}
